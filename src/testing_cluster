workspace()
include("feature_vector.jl")
include("clustering.jl")
include("cluster.jl")

a = FeatureVector(Dict(["hello"=>4, "word"=>0]))
b = FeatureVector(Dict(["hello"=>5, "word"=>0]))
c = FeatureVector(Dict(["hello"=>0, "word"=>4]))
d = FeatureVector(Dict(["hello"=>0, "word"=>5]))
e = FeatureVector(Dict(["hello"=>8, "word"=>4]))
f = FeatureVector(Dict(["hello"=>4, "word"=>8]))
g = FeatureVector(Dict(["kim"=>4, "julia"=>8]))
dict = Dict([1=>a,2=>b,3=>c,4=>d,5=>e,6=>f,7=>g])
array = kmeans(2,dict,cos_similarity,10000)
array[1]
array[2]
